// Basic variable assignment and arithmetic
let x = 5;
let y = 10;
let sum = x + y;
print(sum);

// If-else statement
if (sum > 10) {
    print("Sum is greater than 10");
} else {
    print("Sum is not greater than 10");
}

// While loop
let i = 0;
while (i < 3) {
    print(i);
    i = i + 1;
}

// Function definition and call
let add = fn(a, b) { return a + b; };
let result = add(5, 3);
print(result);
